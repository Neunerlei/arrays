{
    "name": "neunerlei/arrays",
    "description": "A package that provides a multitude of tools to traverse, sort and modify arrays in your daily routine",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "LABOR.digital",
            "email": "info@labor.tools",
            "homepage": "https://www.labor.digital",
            "role": "Company"
        },
        {
            "name": "Martin Neundorfer",
            "email": "code@neunerlei.eu",
            "homepage": "https://www.neunerlei.eu",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/neunerlei/arrays/issues",
        "source": "https://github.com/neunerlei/arrays",
        "docs": "https://arrays.neunerlei.eu/"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "neunerlei/dbg": "^1.8"
    },
    "require": {
        "php": "^7.3||^8.0",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Neunerlei\\Arrays\\": "Classes"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit Tests",
        "test-cover-clover": "XDEBUG_MODE=coverage phpunit --coverage-clover Tests/Coverage.xml",
        "test-cover": "XDEBUG_MODE=coverage phpunit Tests --coverage-html Tests/Coverage"
    },
    "version": "3.3.2"
}
